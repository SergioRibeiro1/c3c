lado1 = 23;
lado2 = 23;
lado3 = 25;

if(lado1 == lado2 && lado1 == lado3){
    console.log("Seu triangulo é o Equilátero, possui todos os lados iguais.");
}else if (lado1 == lado2 || lado1 == lado3 || lado2 == lado3){
    console.log("Seu triangulo é o Isósceles, possui dois lados iguais.");
}else{
    console.log("Seu triangulo é o Escaleno, não possui nenhum lado igual.");
}
